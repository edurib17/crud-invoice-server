<?xml version="1.0" encoding="utf-8"?><databaseChangeLog        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd"        >    <!--        Added the entity Invoice and Invoice Item.    -->    <changeSet id="20231205174100-0" author="admin">        <createTable tableName="invoice">            <column name="id" type="UUID">                <constraints primaryKey="true" nullable="false"/>            </column>            <column name="date" type="date">                <constraints nullable="false"/>            </column>            <column name="description" type="varchar(255)">                <constraints nullable="false"/>            </column>            <column name="invoice_number" type="varchar(255)">                <constraints nullable="false"/>            </column>            <column name="client" type="varchar(255)"/>        </createTable>    </changeSet>    <changeSet id="20231205174100-1" author="admin">        <createTable tableName="invoice_item">            <column name="id" type="UUID">                <constraints primaryKey="true" nullable="false"/>            </column>            <column name="invoice_id" type="UUID">                <constraints nullable="false"/>            </column>            <column name="name" type="varchar(255)">                <constraints nullable="false"/>            </column>            <column name="quantity" type="float"/>            <column name="unit_price" type="decimal(10,2)"/>        </createTable>    </changeSet>    <changeSet id="20231205174100-2" author="admin">        <addForeignKeyConstraint baseColumnNames="invoice_id"                                 baseTableName="invoice_item"                                 constraintName="fk_invoice_item_id"                                 referencedColumnNames="id"                                 referencedTableName="invoice"/>    </changeSet></databaseChangeLog>